%table.table.table-striped.table-hover
  %thead
    %th
    %th User
    %th Role
    %th Internship
    %th Recruited
    %th Employment
    %th Project
    %th Email
    %th Phone
    %th Github
  %tbody
  - users.each do |user|
    %tr
      %td= user.gravatar_image(size: 40)
      %td= user.link
      %td= user.role || "-"
      %td= user.internship || "-"
      %td= user.recruited || "-"
      %td= user.employment || "-"
      %td= user.project_link || "-"
      %td= mail_to user.email, user.email
      %td= user.phone || "-"
      %td= user.github_link
